<view class="page">
  <wux-search-bar clear maxlength="8" />
  <block wx:for="{{projects}}" wx:key="projectId">
    <project-card projectId="{{item.taskId}}" projectName='{{item.projectContent}}' startTime="{{item.startDate}}" endTime="{{item.endDate}}" remainingDay='5' personNum = '2' bindtap='viewDetail' data-projectId="{{item.taskId}}"/>
    </block>
    <view class='card-space'></view>
</view>
<movable-button />
